admin = 5143389125  # Enter Your ID Here

markup = [[InlineKeyboardButton(text='↩️ Reply User', callback_data=f'/replyUser {message.chat.id}')]]
markup = InlineKeyboardMarkup(markup)

bot.sendMessage(
    chat_id=admin,
    text=f"*📩 New Support Message From:\n\n"
         f"➡️ Name: {message.chat.first_name}\n"
         f"➡️ Username: @{message.chat.username}\n"
         f"➡️ ID: {message.chat.id}\n\n"
         f"📝 Message: {message.text}*",
    parse_mode="markdown",
    reply_markup=markup
)

