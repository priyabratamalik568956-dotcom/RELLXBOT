    # Get UTR (previously saved)
    utr = User.getData("utr")

    # Check if user has sent a photo
    if message.photo:
        buttons = [
            [InlineKeyboardButton("Send File ğŸ—ƒï¸", callback_data=f"/approvedepo {message.chat.id}"),
             InlineKeyboardButton("Decline", callback_data=f"/declinedepo {message.chat.id}")]
        ]
        M = InlineKeyboardMarkup(buttons)

        # Send photo to admin
        bot.sendPhoto(
            chat_id=5143389125,
