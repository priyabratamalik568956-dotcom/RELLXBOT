1 command start 
if message.text == "/start":
    amount = 0

    keyboard = {
        "keyboard": [
            [{"text": "One Time Setup ❤️‍🔥"}],
            [{"text": "Headshot 🚨"}, {"text": "Location 🌍"}],
            [{"text": "📞 Support"}, {"text": "📽 How to Use"}]
        ],
        "resize_keyboard": True
    }

    # Step 1: Send initial progress message
    updating = bot.sendMessage(
        chat_id=message.chat.id,
        text="⏳ Updating..."
    )
    msg_id = updating.message_id

    # Step 2: Progress bar frames
    progress_frames = [
        ("🔁 ▰▱▱▱▱▱▱▱▱▱ 0%", 0.3),
        ("🔁 ▰▰▱▱▱▱▱▱▱▱ 10%", 0.3),
        ("🔁 ▰▰▰▰▱▱▱▱▱▱ 25%", 0.3),
        ("🔁 ▰▰▰▰▰▱▱▱▱▱ 40%", 0.3),
        ("🔁 ▰▰▰▰▰▰▰▱▱▱ 56%", 0.3),
        ("🔁 ▰▰▰▰▰▰▰▰▱▱ 67%", 0.3),
        ("🔁 ▰▰▰▰▰▰▰▰▰▱ 88%", 0.3),
        ("🔁 ▰▰▰▰▰▰▰▰▰▰ 100%", 0.4),
        ("✅ Done", 0.5)
    ]

    for frame, delay in progress_frames:
        bot.editMessageText(
            chat_id=message.chat.id,
            message_id=msg_id,
            text=frame
        )
        time.sleep(delay)

    # Step 3: Delete progress bar message
    bot.deleteMessage(chat_id=message.chat.id, message_id=msg_id)

    # Step 4: Send photo + stylish welcome
    bot.sendPhoto(
        chat_id=message.chat.id,
        photo="https://envs.sh/hvz.jpg/IMG20250810701.jpg",
        caption=(
            "<b>🪴 WELCOME TO FF PANEL BOT 🪴</b>\n\n"
            "🔥 We Provide <u>Cheapest</u> And <u>Most Powerful</u> Panel In The World 🌎\n\n"
            "✨ <b>Features Include:</b>\n"
            "• Ultra Fast Service 🚀\n"
            "• 24/7 Support 🛡\n"
            "• Trusted By Thousands ❤️\n\n"
            "||Tap Below To Unlock Premium Experience||"
        ),
        parse_mode="HTML",
        reply_markup=keyboard
    )

    # Step 5: Referral Bonus Logic
    if User.getData("referbonus") == None:
        if User.getData("referrer") != None:
            refer = User.getData("referrer")
            libs.Resources.anotherRes("balance", user=refer).add(amount)
            bot.replyText(
                refer,
                f"<i>You have received {amount} Points from {message.from_user.first_name}</i>"
            )
            User.saveData("referbonus", True)
            
