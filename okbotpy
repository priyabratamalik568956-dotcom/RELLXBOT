            9
try:
    # Delete user's original message
    bot.deleteMessage(chat_id=u, message_id=message.message_id)

    # Get UTR (previously saved)
    utr = User.getData("utr")

    # Check if user has sent a photo
    if message.photo:
        buttons = [
            [InlineKeyboardButton("Send File 🗃️", callback_data=f"/approvedepo {message.chat.id}"),
             InlineKeyboardButton("Decline", callback_data=f"/declinedepo {message.chat.id}")]
        ]
        M = InlineKeyboardMarkup(buttons)

        # Send photo to admin
        bot.sendPhoto(
            chat_id=5143389125,
            photo=message.photo[-1].file_id,  # use highest resolution
            caption=f"🆕 New Deposit: {utr}",
            reply_markup=M
        )

        # Notify user
        Bot.sendMessage("<b>Your Deposit Sent For Verification ✅</b>")
    else:
        Bot.sendMessage("❌ Please send a valid screenshot.")
except Exception as e:
    bot.sendMessage(f"⚠️ Error: {str(e)}")
    
    
